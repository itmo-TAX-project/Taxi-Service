{
  "Kestrel": {
    "Endpoints": {
      "gRPC": {
        "Url": "http://0.0.0.0:8020",
        "Protocols": "Http2"
      }
    }
  },
  "Postgres": {
    "Host": "localhost",
    "Port": 5300,
    "Database": "postgres",
    "Username": "postgres",
    "Password": "postgres",
    "SslMode": "Prefer"
  },
  "MessagePersistence": {
    "SchemaName": "message_persistence",
    "Publisher": {
      "Default": {
        "BatchSize": 10,
        "PollingDelay": "00:00:00.500"
      }
    }
  },
  "Kafka": {
    "Host": "localhost:8001",
    "Producers": {
      "TaxiDriverCreatedMessage": {
        "Topic": "taxi.driver.created",
        "Outbox": {
          "BatchSize": 100,
          "PollingDelay": "00:00:00.500"
        }
      },
      "TaxiDriverStatusChangedMessage": {
        "Topic": "taxi.driver.status.changed",
        "Outbox": {
          "BatchSize": 100,
          "PollingDelay": "00:00:00.500"
        }
      },
      "TaxiVehicleChangedMessage": {
        "Topic": "taxi.vehicle.changed",
        "Outbox": {
          "BatchSize": 100,
          "PollingDelay": "00:00:00.500"
        }
      }
    },
    "Consumers": {
      "AccountDeletedMessage": {
        "IsDisabled": false,
        "Topic": "account.deleted",
        "Group": "Taxi-Service",
        "Inbox": {
          "BatchSize": 10,
          "PollingDelay": "00:00:00.500"
        }
      },
      "TaxiDriverStatusChangeMessage": {
        "IsDisabled": false,
        "Topic": "taxi.driver.status.change",
        "Group": "Taxi-Service",
        "Inbox": {
          "BatchSize": 10,
          "PollingDelay": "00:00:00.500"
        }
      },
      "TaxiDriverCreateMessage": {
        "IsDisabled": false,
        "Topic": "taxi.driver.create",
        "Group": "Taxi-Service",
        "Inbox": {
          "BatchSize": 10,
          "PollingDelay": "00:00:00.500"
        }
      },
      "TaxiDriverVehicleChangeMessage": {
        "IsDisabled": false,
        "Topic": "taxi.vehicle.change",
        "Group": "Taxi-Service",
        "Inbox": {
          "BatchSize": 10,
          "PollingDelay": "00:00:00.500"
        }
      },
      "TaxiVehicleCreateMessage": {
        "IsDisabled": false,
        "Topic": "taxi.vehicle.create",
        "Group": "Taxi-Service",
        "Inbox": {
          "BatchSize": 10,
          "PollingDelay": "00:00:00.500"
        }
      }
    }
  }
}